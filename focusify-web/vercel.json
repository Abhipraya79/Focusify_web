{
    "version": 2,
    "builds": [
      {
        "src": "focusify-web/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
    {"handle": "filesystem"},
      
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  